{
  "concurrency": "4",
  "globalPassThroughEnv": ["GITHUB_TOKEN"],
  "tasks": {
    "build": {
      "inputs": [
        "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "package.json"
      ],
      "outputs": [
        "dist/**",
        ".vercel/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "cache": false
    },
    "debug": {
      "persistent": true,
      "cache": false
    },
    "test:dev": {
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "coverage": {
      "inputs": [
        "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "package.json"
      ],
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^test"
      ],
      "cache": true
    },
    "format": {
      "inputs": [
        "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "package.json"
      ],
      "cache": true
    },
    "lint": {
      "inputs": [
        "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "package.json"
      ],
      "cache": true
    },
    "precommit": {
      "inputs": [
        "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "package.json"
      ],
      "cache": false
    },
    "test": {
      "inputs": [
        "src/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "test/**/*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}",
        "package.json"
      ],
      "cache": true
    }
  }
}
