{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "./node_modules/.bin/astro dev",
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Worker",
      "program": "${workspaceFolder}/packages/tictactoe-demo/dist/esm/index.js",
      "runtimeArgs": [
        "--inspect-brk"
      ],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/packages/**/dist/**/*.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "autoAttachChildProcesses": true,
      "preLaunchTask": "build"
    }
  ]
}
